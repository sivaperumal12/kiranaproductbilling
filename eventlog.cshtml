@{
    ViewBag.title = "Event Logs";
}
<section class="content">
    <div class="row">
        <div class="col-xs-12">

            <div class="box">
                <div class="box-header">
                    <h3 class="box-title">Event Logs</h3>
                </div>
                <!-- /.box-header -->
                <div class="box-body">
                    <table id="example1" class="table table-bordered table-striped">
                        <thead>
                            <tr>
                                <th>S.No</th>

                                <th>Name</th>
                                <th>Message</th>
                                <th>Done By</th>
                                

                                <th>Date</th>



                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var itm in ViewBag.usr)
                            {
                                <tr>
                                    <td>@itm.ID</td>
                                    <td>@itm.name</td>
                                    <td>@itm.message</td>

                                    <td>@itm.uid</td>

                                   
                                    <td>@itm.cdate</td>

                                </tr>
                            }
                        </tbody>

                    </table>
                </div>
                <!-- /.box-body -->
            </div>
            <!-- /.box -->
            <!-- /.box-body -->
        </div>
        <!-- /.box -->
    </div>

    <script>
        var id; // global variable
         var sts = '@ViewBag.sts';
    $(document).ready(function () {
        if (sts != "") {
            alert(sts);
        }
        });

       

    </script>
</section>


